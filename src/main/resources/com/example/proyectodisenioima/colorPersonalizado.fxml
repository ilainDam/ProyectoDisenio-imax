   </stylesheets>
   <children>
      <fx:include source="barraSuperior.fxml" />
      <Label alignment="CENTER" prefHeight="150.0" prefWidth="1980.0" text="Seleccionador de colores personalizados" textAlignment="CENTER">
         <styleClass>
            <String fx:value="tituloApartado" />
            <String fx:value="fuente62" />
         </styleClass>
      </Label>
      <Label alignment="CENTER" contentDisplay="BOTTOM" prefHeight="110.0" prefWidth="1980.0" text="Color principal del fondo">
         <graphic>
            <ColorPicker />
         </graphic>
         <styleClass>
            <String fx:value="seleccionador" />
            <String fx:value="fuente32" />
         </styleClass>
      </Label>
      <Label alignment="CENTER" contentDisplay="BOTTOM" prefHeight="110.0" prefWidth="1980.0" text="Color secundario del fondo">
         <graphic>
            <ColorPicker />
         </graphic>
         <styleClass>
            <String fx:value="seleccionador" />
            <String fx:value="fuente32" />
         </styleClass>
      </Label>
      <Label alignment="CENTER" contentDisplay="BOTTOM" prefHeight="110.0" prefWidth="1980.0" text="Color principal de resaltado">
         <graphic>
            <ColorPicker />
         </graphic>
         <styleClass>
            <String fx:value="seleccionador" />
            <String fx:value="fuente32" />
         </styleClass>
      </Label>
      <Label alignment="CENTER" contentDisplay="BOTTOM" prefHeight="110.0" prefWidth="1980.0" text="Color secundario del resaltado">
         <graphic>
            <ColorPicker />
         </graphic>
         <styleClass>
            <String fx:value="seleccionador" />
            <String fx:value="fuente32" />
         </styleClass>
      </Label>
      <Label alignment="CENTER" contentDisplay="BOTTOM" prefHeight="110.0" prefWidth="1980.0" text="Color terciario del resaltado">
