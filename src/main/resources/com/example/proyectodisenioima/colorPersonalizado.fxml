      </Label>
      <Label alignment="CENTER" contentDisplay="BOTTOM" prefHeight="110.0" prefWidth="1980.0" text="Color principal de la fuente">
         <graphic>
            <ColorPicker />
         </graphic>
         <styleClass>
            <String fx:value="seleccionador" />
            <String fx:value="fuente32" />
         </styleClass>
      </Label>
      <Label alignment="CENTER" contentDisplay="BOTTOM" prefHeight="110.0" prefWidth="1980.0" text="Color secundario de la fuente">
         <graphic>
            <ColorPicker />
         </graphic>
         <styleClass>
            <String fx:value="seleccionador" />
            <String fx:value="fuente32" />
         </styleClass>
      </Label>
